
[
  {
    "selector": "node",
    "style": {
      "label": "data(name)",
      "text-valign": "center",
      "text-halign": "center",
      "color": "#ffffff",
      "text-outline-width": 2,
      "text-outline-color": "#34495e",
      "background-color": "#3498db",
      "shape": "ellipse",
      "font-size": 12
    }
  },
  {
    "selector": "node[class='type']",
    "style": {
      "shape": "round-rectangle",
      "background-color": "#2ecc71",
      "text-outline-color": "#27ae60"
    }
  },
  {
    "selector": "node[class='individual']",
    "style": {
      "shape": "ellipse",
      "background-color": "#9b59b6",
      "text-outline-color": "#8e44ad"
